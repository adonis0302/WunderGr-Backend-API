prefer-workspace-packages=true # This avoids downloading packages even when a newer version is available in the registry
save-workspace-protocol=false # This prevents the examples to have the `workspace:` prefix
link-workspace-packages=true # This links the packages to the workspace instead of copying them
strict-peer-dependencies=false # If this is enabled, commands will fail if there is a missing or invalid peer dependency in the tree.
fetch-retries="10"
engine-strict=true
